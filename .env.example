# SSH Server Configuration
SSH_PORT=2222

# Domain for tunnel service
DOMAIN=unggahin.com

# HTTP/HTTPS ports
HTTP_PORT=80
HTTPS_PORT=443

# Path to SSH host key (will be generated if not exists)
HOST_KEY_PATH=./ssh_host_key

# Certificate cache directory for Let's Encrypt
CERT_CACHE_DIR=./certs

# Let's Encrypt email (optional, for certificate renewal notifications)
LETSENCRYPT_EMAIL=your-email@example.com

# Request timeout (e.g., 30s, 1m, 90s)
REQUEST_TIMEOUT=30s

# Enable HTTPS (true/false)
ENABLE_HTTPS=true
